# logging properties
logging.level.org.springframework.web=DEBUG

# server properties
server.port=9090

# h2 properties
spring.h2.console.enabled=true
spring.h2.console.path=/h2

# primary data source
spring.datasource.url=jdbc:h2:file:E:/Backup/JavaPrep/practiceProjects/Cerebro/src/main/resources/cerebro;MV_STORE=FALSE
spring.datasource.username=cerebro
spring.datasource.password=cerebro
spring.datasource.driver-class-name=org.h2.Driver
spring.jpa.database-platform=org.hibernate.dialect.H2Dialect

## JPA properties
spring.jpa.show-sql=true
#This is actually a shortcut for the "hibernate.hbm2ddl.auto" property. Default to "create-drop" when using an embedded database, "none" otherwise.
spring.jpa.hibernate.ddl-auto=none